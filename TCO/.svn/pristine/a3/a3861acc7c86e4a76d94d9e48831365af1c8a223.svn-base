<%@ page language="java" contentType="text/html; charset=UTF-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<!DOCTYPE html>
<html>
<head>
<title>Insert title here</title>
<style type="text/css">
	input[type=text]{
	readonly
	}
	label{
	text-align: center;
	}
</style>
</head>
<body>
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h1 class="mb-sm-0 font-size-20"><i class="bx bx-dollar"></i>매출 상세정보</h1>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">Sales</a></li>
                    <li class="breadcrumb-item active">Detail</li>
                </ol>
           
            </div>

        </div>
    </div>
</div>
<!-- end page title -->

<!-- form태그 시작 -->
<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">
				
                <div class="d-flex gap-3">
                	<h4 class="card-title">매출정보</h4>
				</div>
            	</div>
			<hr><br>
                <div class="mb-3 row">
                    <label for="example-text-input" class="col-md-2 col-form-label">매출번호</label>
                    <div class="col-md-4">
                        <input class="form-control detail" type="text" value="${slsVO.slsId}" />
                    </div>
                </div>
                <div class="mb-3 row">
                    <label class="col-md-2 col-form-label">지점아이디</label>
                    <div class="col-md-6">
                        <input class="form-control detail" type="tel" value="${slsVO.brncofcId}" />
                    </div>
                </div>                 
                <div class="mb-3 row">
                    <label for="example-email-input" class="col-md-2 col-form-label">지점명</label>
                    <div class="col-md-6">
                        <input class="form-control detail" type="email" value="${slsVO.brncofcNm}" />
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-week-input" class="col-md-2 col-form-label">재고 </label>
                    <div class="col-md-6">
                        <input class="form-control detail" type="text" value="${slsVO.streStckQty}" />
                    </div>     
                </div>
                <div class="mb-3 row">
                    <label for="example-month-input" class="col-md-2 col-form-label">안전재고 </label>
                    <div class="col-md-6">
                        <input class="form-control detail" type="text" value="${slsVO.streStckSftyQty}" />
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-month-input" class="col-md-2 col-form-label">등록일 </label>
                    <div class="col-md-6">
                        <input class="form-control detail" type="text" value="${slsVO.slsDt}" />
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-url-input" class="col-md-2 col-form-label">prcAmt</label>
                    <div class="col-md-6">
                        <input class="form-control detail" type="text" value="${slsVO.prcAmt}"/>
                    </div>
                </div>
            </div>
        </div>
    </div> <!-- end col -->
</body>
</html>