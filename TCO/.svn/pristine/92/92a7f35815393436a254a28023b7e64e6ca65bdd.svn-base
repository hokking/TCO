<%@ page language="java" contentType="text/html; charset=UTF-8" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<!-- start page title -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
            <h4 class="mb-sm-0 font-size-18">고객 등록</h4>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a href="javascript: void(0);">Forms</a></li>
                    <li class="breadcrumb-item active">Form Elements</li>
                </ol>
            </div>

        </div>
    </div>
</div>
<!-- end page title -->

<div class="row">
    <div class="col-12">
        <div class="card">
            <div class="card-body">

                <h4 class="card-title">고객 정보</h4><br>

                <div class="mb-3 row">
                    <label for="example-text-input" class="col-md-2 col-form-label">고객명</label>
                    <div class="col-md-10">
                        <input class="form-control" type="text" value="Artisanal kale"
                            id="example-text-input">
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-tel-input" class="col-md-2 col-form-label">전화번호</label>
                    <div class="col-md-10">
                        <input class="form-control" type="tel" value="" placeholder="000-0000-0000"
                            id="example-tel-input">
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-email-input" class="col-md-2 col-form-label">이메일</label>
                    <div class="col-md-10">
                        <input class="form-control" type="email" value="" placeholder="tco1234@tco.com"
                            id="example-email-input">
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-text-input" class="col-md-2 col-form-label">우편번호</label>
                    <div class="col-md-10">
                        <input class="form-control" type="text" value="Artisanal kale"
                            id="example-text-input">
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-text-input" class="col-md-2 col-form-label">주소</label>
                    <div class="col-md-10">
                        <input class="form-control" type="text" value="Artisanal kale"
                            id="example-text-input">
                    </div>
                </div>
				<div class="mb-3 row">
                    <label for="example-text-input" class="col-md-2 col-form-label">상세주소</label>
                    <div class="col-md-10">
                        <input class="form-control" type="text" value="Artisanal kale"
                            id="example-text-input">
                    </div>
                </div>
                <div class="mb-3 row">
                    <label for="example-date-input" class="col-md-2 col-form-label">생년월일</label>
                    <div class="col-md-10">
                        <input class="form-control" type="date" value="2019-08-19"
                            id="example-date-input">
                    </div>
                </div>
                <div class="mb-3 row">
                    <label class="col-md-2 col-form-label">결혼여부</label>
                    <div class="col-md-10">
                        <div class="form-check form-radio-info mt-2">
	                        <input class="form-check-input" type="radio" name="formRadios" id="formRadios1" checked>
	                        <label class="form-check-label" for="formRadios1">
	                        	기혼
	                        </label>
	                    </div>
                        <div class="form-check form-radio-info mt-2">
                            <input class="form-check-input" type="radio" name="formRadios" id="formRadios2">
                            <label class="form-check-label" for="formRadios2">
                            	미혼
                            </label>
                        </div>
                    </div>
                </div>
                <div class="mb-3 row">
                    <label class="col-md-2 col-form-label">성별</label>
                    <div class="col-md-10">
                        <div class="form-check form-radio-info mt-2">
	                        <input class="form-check-input" type="radio" name="formRadios1" id="formRadios3" checked>
	                        <label class="form-check-label" for="formRadios3">
	                        	여성
	                        </label>
	                    </div>
                        <div class="form-check form-radio-info mt-2">
                            <input class="form-check-input" type="radio" name="formRadios1" id="formRadios4">
                            <label class="form-check-label" for="formRadios4">
                            	남성
                            </label>
                        </div>
                    </div>
                </div>
                <div class="mb-3 row">
                    <label class="col-md-2 col-form-label">마케팅수신여부</label>
                    <div class="col-md-10">
                        <div class="form-check form-check-info mt-2">
                            <input class="form-check-input" type="checkbox" id="formCheckcolor3" checked="">
                            <label class="form-check-label" for="formCheckcolor3">
                                	마케팅 수신여부 동의
                            </label>
                        </div>
                    </div>
                </div>
				
            </div>
		<button type="button" class="btn btn-info waves-effect waves-light">등록</button>
        </div>
    </div> <!-- end col -->
</div>
<!-- end row -->